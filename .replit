run = "python tts_bot.py"
language = "python3"
entrypoint = "tts_bot.py"

[nix]
channel = "stable-22_11"

[env]
PYTHONUNBUFFERED = "1"

[packager]
language = "python3"

[packager.features]
enabledForHosting = false
packageSearch = true
guessImports = true

[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = "pylsp"

[deployment]
run = ["sh", "-c", "python tts_bot.py"]
deploymentTarget = "cloudrun"
